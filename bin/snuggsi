#!/usr/bin/env bash
# http://www.tldp.org/LDP/abs/html/parameter-substitution.html

export NAME=$1
PARAMS=$@

echo "All $ALL_PARAMS"

for PARAM in $PARAMS
do
echo $PARAM
done


echo "Snuggsi cli"

echo "Component name $NAME"

./bin/style ${NAME}

# Commands
# init
# component :name
# style :name
# browse :name
# lint :name
# test :name
# compress :name
# serve [:name]

