const
  contents =
    require ('fs')
      .readFileSync
         ('./dist/snuggsi.es', {encoding: 'UTF-8'})

const
  compiler =
    require ('google-closure-compiler-js')

const options = {
    jsCode: [{src: contents}],
    languageIn: 'ES6'
};

const out = compiler.compile(options);

// if (out.errors && out.errors.length) {
//     console.error(out.errors);
//     throw new Error(`Minification failed for ${file}`);
// }
console.log( out.compiledCode);
