{
  "name": "snuggsi",
  "version": "0.7.25",
  "author": "Snuggs",
  "description": "Easy Web Components in 1KB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devpunks/snuggsi/issues"
  },
  "homepage": "https://github.com/devpunks/snuggsi#readme",
  "engines": {
    "npm": "4.2.0",
    "node": "7.8.x"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/devpunks/snuggsi.git"
  },
  "keywords": [
    "web",
    "components",
    "resource",
    "routing",
    "templates"
  ],
  "main": "./dist/snuggsi.min.es.js",
  "bin": "bin/snuggsi",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "start": "node --harmony ./index.es",
    "postinstall": "bin/publish",
    "package": "bin/package",
    "test": "./bin/test",
    "cover": "tap index.test.es --cov --coverage-report=lcov",
    "compile": "bin/bundle && bin/transpile && bin/shrink && bin/compress && bin/weigh"
  },
  "devDependencies": {
    "devtools-terminal": "*"
  },
  "dependencies": {
    "koa": "*",
    "koa-send": "*",
    "koa-static": "*",
    "tap": "*",
    "tape": "*",
    "tap-nyc": "*",
    "jsdom": "*",
    "buble": "*",
    "uglify-js": "*",
    "uglify-es": "*",
    "buble-tape": "*",
    "browser-sync": "*"
  }
}
